{
  "name": "@dnovicki/dv-components",
  "version": "2.0.108",
  "description": "DV components built with React and Bootstrap",
  "scripts": {
    "test": "jest --verbose",
    "lint": "eslint --ext .js --ext .jsx ./src",
    "build": "webpack",
    "start": "webpack --watch",
    "prepublish": "npm run build",
    "precommit": "npm run lint && npm run test",
    "commitmsg": "validate-commit-msg",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "files": [
    "dist/components.js",
    "dist/components.bundle.js",
    "dist/styles.css"
  ],
  "main": "dist/components.js",
  "author": "David Novicki",
  "license": "ISC",
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.4",
    "@storybook/addon-backgrounds": "^3.4.4",
    "@storybook/addon-console": "^1.0.0",
    "@storybook/addon-info": "^3.4.4",
    "@storybook/addon-knobs": "^3.4.4",
    "@storybook/addon-links": "^3.4.4",
    "@storybook/addon-viewport": "^3.4.4",
    "@storybook/addons": "^3.4.4",
    "@storybook/react": "^3.4.4",
    "autoprefixer": "^7.1.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-async-to-promises": "^1.0.5",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.2.2",
    "eslint": "^4.12.1",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "jest-enzyme": "^4.0.1",
    "node-sass": "^4.7.2",
    "normalize.css": "^8.0.0",
    "polished": "^1.9.2",
    "postcss-animation": "0.0.12",
    "postcss-loader": "^2.0.9",
    "postcss-smart-import": "^0.7.5",
    "precss": "^2.0.0",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.6",
    "sinon": "^4.1.2",
    "storybook-addon-a11y": "^3.1.9",
    "storybook-react-router": "^1.0.1",
    "style-loader": "^0.19.0",
    "styled-system": "^2.2.5",
    "svg-inline-loader": "^0.8.0",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "validate-commit-msg": "^2.14.0",
    "webpack": "^3.8.1"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jestconfig.js"
    ],
    "moduleFileExtensions": [
      "jsx",
      "js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "dependencies": {
    "@trainline/react-skeletor": "^1.0.2",
    "clean-tag": "^1.0.4",
    "lodash": "^4.17.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-spring": "^5.1.10",
    "styled-components": "^3.2.6",
    "system-components": "^2.0.3"
  }
}
